{
  "name": "carpartyclient",
  "version": "0.0.1",
  "description": "The controller client for the game CarParty! This runs in a webbrowser on your handheld device.",
  "scripts": {
    "build": "webpack --env MODE=production",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "start": "webpack serve --env MODE=development --port 4200",
    "start:unsafe": "npm run start -- --host=0.0.0.0 --allowed-hosts all",
    "test:watch": "karma start",
    "test": "karma start --single-run",
    "test:headless": "karma start --single-run"
  },
